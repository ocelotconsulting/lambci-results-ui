{
  "name": "lambci-results-ui",
  "version": "0.1.0",
  "description": "UI for viewing lambci results in S3",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf public && rm -rf dist && mkdir public",
    "dev": "nodemon --watch . --ignore test --ignore src -e js,html server.js",
    "build:copy-images": "cp -r images public/images",
    "build:copy-html": "cp index.html public/index.html",
    "build:webpack": "NODE_ENV=production webpack",
    "build": "npm run clean && npm-run-all build:*",
    "build-lambda": "webpack --config webpack.lambda.config.js",
    "deploy-lambda": "rm -rf dist && npm run build-lambda && node build/deployLambda.js",
    "deploy-ui": "NODE_ENV=production npm run build && node build/deployUI.js",
    "start": "NODE_ENV=production node server",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "test": "mocha",
    "test:dev": "mocha -w --reporter min"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/ocelotconsulting/lambci-results-ui.git"
  },
  "keywords": [
    "lambda",
    "ci",
    "ui"
  ],
  "author": "John O'Malley",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ocelotconsulting/lambci-results-ui/issues"
  },
  "homepage": "https://github.com/ocelotconsulting/lambci-results-ui#readme",
  "dependencies": {
    "aws-sdk": "~2.390.0",
    "body-parser": "~1.18.3",
    "dynamodb-doc": "~1.0.0",
    "express": "~4.16.4",
    "lodash": "~4.17.4",
    "moment": "~2.23.0",
    "mime-types": "~2.1.14",
    "underscore.string": "~3.3.4"
  },
  "devDependencies": {
    "@babel/core": "~7.2.0",
    "@babel/plugin-proposal-class-properties": "~7.2.3",
    "@babel/plugin-transform-runtime": "~7.2.0",
    "@babel/polyfill": "~7.2.5",
    "@babel/preset-env": "~7.2.3",
    "@babel/preset-react": "~7.0.0",
    "@babel/register": "~7.0.0",
    "@babel/runtime": "~7.2.0",
    "archiver": "~3.0.0",
    "babel-eslint": "~10.0.1",
    "babel-loader": "~8.0.5",
    "bootstrap": "~3.3.7",
    "chai": "~4.2.0",
    "chai-as-promised": "~7.1.1",
    "classnames": "~2.2.5",
    "css-loader": "~2.1.0",
    "enzyme": "~3.8.0",
    "enzyme-adapter-react-16": "~1.7.1",
    "eslint": "~5.12.0",
    "eslint-config-standard": "~12.0.0",
    "eslint-plugin-import": "~2.14.0",
    "eslint-plugin-node": "~8.0.1",
    "eslint-plugin-promise": "~4.0.1",
    "eslint-plugin-react": "~7.12.4",
    "eslint-plugin-standard": "~4.0.0",
    "file-loader": "~3.0.1",
    "font-awesome": "~4.7.0",
    "js-cookie": "~2.2.0",
    "json-loader": "~0.5.4",
    "less": "~3.9.0",
    "less-loader": "~4.1.0",
    "mocha": "~5.2.0",
    "nodemon": "~1.18.9",
    "npm-run-all": "~4.1.5",
    "prop-types": "~15.6.2",
    "proxyquire": "~2.1.0",
    "query-string": "~6.2.0",
    "react": "~16.7.0",
    "react-dom": "~16.7.0",
    "react-redux": "~6.0.0",
    "react-router": "~3.0.2",
    "react-test-renderer": "~16.7.0",
    "redux": "~4.0.1",
    "redux-logger": "~3.0.6",
    "redux-thunk": "~2.3.0",
    "sinon": "~7.2.2",
    "sinon-chai": "~3.3.0",
    "style-loader": "~0.23.1",
    "superagent": "~4.1.0",
    "url-loader": "~1.1.2",
    "webpack": "~4.28.4",
    "webpack-cli": "~3.2.1",
    "webpack-dev-middleware": "~3.5.1",
    "webpack-hot-middleware": "~2.24.3"
  }
}
