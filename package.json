{
  "name": "lambci-results-ui",
  "version": "1.0.0",
  "description": "UI for viewing lambci results in S3",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf public && rm -rf lambda && mkdir public",
    "dev": "NODE_ENV=development supervisor -w server.js,index.html,enableDevMode.js,api node server",
    "build:copy-images": "cp -r images public/images",
    "build:copy-html": "cp index.html public/index.html",
    "build:less": "lessc --clean-css src/styles.less public/styles.css",
    "build:webpack": "NODE_ENV=production webpack",
    "build:lambda": "webpack --config webpack.lambda.config.js",
    "build": "npm run clean && npm-run-all build:*",
    "start": "NODE_ENV=production node server",
    "test": "mocha",
    "test:dev": "mocha -w"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/ocelotconsulting/lambci-results-ui.git"
  },
  "keywords": [
    "lambda",
    "ci",
    "ui"
  ],
  "author": "John O'Malley",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ocelotconsulting/lambci-results-ui/issues"
  },
  "homepage": "https://github.com/ocelotconsulting/lambci-results-ui#readme",
  "engines": {
    "node": ">=6.7.0",
    "npm": ">=3.0.0"
  },
  "dependencies": {
    "aws-sdk": "~2.7.0",
    "body-parser": "~1.15.2",
    "dynamodb-doc": "~1.0.0",
    "express": "~4.14.0",
    "lodash": "~4.16.6",
    "moment": "~2.16.0",
    "underscore.string": "~3.3.4"
  },
  "devDependencies": {
    "babel-core": "~6.18.2",
    "babel-eslint": "~7.1.0",
    "babel-loader": "~6.2.5",
    "babel-plugin-transform-class-properties": "~6.18.0",
    "babel-plugin-transform-runtime": "~6.15.0",
    "babel-polyfill": "~6.16.0",
    "babel-preset-es2015": "~6.18.0",
    "babel-preset-react": "~6.16.0",
    "babel-preset-stage-0": "~6.16.0",
    "babel-runtime": "~6.18.0",
    "bootstrap": "~3.3.7",
    "chai": "~3.5.0",
    "chai-as-promised": "~6.0.0",
    "classnames": "~2.2.5",
    "enzyme": "~2.6.0",
    "eslint": "~3.9.1",
    "eslint-config-standard": "^6.2.0",
    "eslint-plugin-babel": "^3.3.0",
    "eslint-plugin-promise": "^3.3.0",
    "eslint-plugin-react": "^6.4.1",
    "eslint-plugin-standard": "^2.0.1",
    "js-cookie": "~2.1.3",
    "json-loader": "~0.5.4",
    "less": "~2.7.1",
    "less-middleware": "~2.2.0",
    "less-plugin-clean-css": "~1.5.1",
    "mocha": "~3.1.2",
    "npm-run-all": "~3.1.1",
    "react": "~15.3.2",
    "react-addons-test-utils": "~15.3.2",
    "react-bootstrap": "~0.30.5",
    "react-dom": "~15.3.2",
    "react-redux": "~4.4.5",
    "react-router": "~2.8.1",
    "redux": "~3.6.0",
    "redux-logger": "~2.7.4",
    "redux-thunk": "~2.1.0",
    "sinon": "~1.17.6",
    "sinon-as-promised": "~4.0.2",
    "sinon-chai": "~2.8.0",
    "superagent": "~2.3.0",
    "superagent-promise": "~1.1.0",
    "supervisor": "~0.11.0",
    "webpack": "~1.13.2",
    "webpack-dev-middleware": "~1.8.4"
  }
}
