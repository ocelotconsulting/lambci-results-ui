{
  "name": "lambci-results-ui",
  "version": "0.1.0",
  "description": "UI for viewing lambci results in S3",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf public && rm -rf dist && mkdir public",
    "dev": "NODE_ENV=development supervisor -w server.js,index.html,dev.js,api node server",
    "build:copy-images": "cp -r images public/images",
    "build:copy-html": "cp index.html public/index.html",
    "build:less": "lessc --clean-css css/styles.less public/styles.css",
    "build:webpack": "NODE_ENV=production webpack",
    "build": "npm run clean && npm-run-all build:*",
    "build-lambda": "webpack --config webpack.lambda.config.js",
    "deploy-lambda": "rm -rf dist && npm run build-lambda && node build/deployLambda.js",
    "deploy-ui": "npm run build && node build/deployUI.js",
    "start": "NODE_ENV=production node server",
    "test": "mocha",
    "test:dev": "mocha -w"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/ocelotconsulting/lambci-results-ui.git"
  },
  "keywords": [
    "lambda",
    "ci",
    "ui"
  ],
  "author": "John O'Malley",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ocelotconsulting/lambci-results-ui/issues"
  },
  "homepage": "https://github.com/ocelotconsulting/lambci-results-ui#readme",
  "engines": {
    "node": ">=6.7.0",
    "npm": ">=3.0.0"
  },
  "dependencies": {
    "aws-sdk": "~2.9.0",
    "body-parser": "~1.16.0",
    "dynamodb-doc": "~1.0.0",
    "express": "~4.14.1",
    "lodash": "~4.17.4",
    "moment": "~2.17.1",
    "mime-types": "~2.1.14",
    "underscore.string": "~3.3.4"
  },
  "devDependencies": {
    "archiver": "~1.3.0",
    "babel-core": "~6.22.1",
    "babel-loader": "~6.2.10",
    "babel-plugin-transform-class-properties": "~6.22.0",
    "babel-plugin-transform-runtime": "~6.22.0",
    "babel-polyfill": "~6.22.0",
    "babel-preset-es2015": "~6.22.0",
    "babel-preset-react": "~6.22.0",
    "babel-preset-stage-0": "~6.22.0",
    "babel-runtime": "~6.22.0",
    "bootstrap": "~3.3.7",
    "chai": "~3.5.0",
    "chai-as-promised": "~6.0.0",
    "classnames": "~2.2.5",
    "decache": "~4.1.0",
    "enzyme": "~2.7.0",
    "js-cookie": "~2.1.3",
    "json-loader": "~0.5.4",
    "less": "~2.7.2",
    "less-middleware": "~2.2.0",
    "less-plugin-clean-css": "~1.5.1",
    "mocha": "~3.2.0",
    "mockery": "~2.0.0",
    "npm-run-all": "~4.0.1",
    "query-string": "~4.3.1",
    "react": "~15.4.2",
    "react-addons-test-utils": "~15.4.2",
    "react-dom": "~15.4.2",
    "react-redux": "~5.0.2",
    "react-router": "~3.0.2",
    "redux": "~3.6.0",
    "redux-logger": "~2.7.4",
    "redux-thunk": "~2.2.0",
    "sinon": "~1.17.7",
    "sinon-as-promised": "~4.0.2",
    "sinon-chai": "~2.8.0",
    "superagent": "~3.4.1",
    "supervisor": "~0.12.0",
    "webpack": "~2.2.1",
    "webpack-dev-middleware": "~1.9.0",
    "webpack-hot-middleware": "~2.16.1"
  }
}
